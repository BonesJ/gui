}

QTabBar::tab:top:selected,
QTabBar::tab:bottom:selected {
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #4771b3, stop:1 #638dcf);
    border-right-color: black;
